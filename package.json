{
  "name": "youtube-playlist-export",
  "version": "1.0.0",
  "description": "A command line app for exporting YouTube playlist data into a CSV/JSON file",
  "type": "module",
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "youtube",
    "playlist",
    "youtube api",
    "youtube playlist",
    "csv",
    "json",
    "download",
    "cli",
    "command line"
  ],
  "license": "MIT",
  "author": {
    "name": "AnsonH",
    "email": "ansonheung2000@gmail.com",
    "url": "https://ansonheung.me"
  },
  "bin": {
    "ytpl-export": "./cli.js",
    "youtube-playlist-export": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnsonH/youtube-playlist-export.git"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,md}",
    "lint": "eslint **/*.js --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "conf": "^10.0.2",
    "inquirer": "^8.1.2",
    "json2csv": "^5.0.6",
    "ora": "^6.0.0",
    "read-pkg": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,md}": [
      "prettier --write"
    ],
    "**/*.js": [
      "eslint --fix"
    ]
  }
}
